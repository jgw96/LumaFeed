{
  "name": "feeding-tracker",
  "version": "0.1.0",
  "type": "module",
  "description": "A Progressive Web App for tracking feeding",
  "main": "index.html",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:analyze": "tsc && vite build --mode analyze",
    "preview": "vite preview",
    "capture:screenshots": "node ./scripts/generate-manifest-screenshots.mjs",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "format": "prettier --write \"**/*.{ts,js,json,html,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,js,json,html,css,md}\"",
    "optimize:images": "node ./scripts/optimize-images.mjs --verbose",
    "optimize:check": "node ./scripts/optimize-images.mjs --dry-run --verbose",
    "bundle:check": "node ./scripts/check-bundle-size.mjs"
  },
  "keywords": [
    "pwa",
    "lit",
    "web-components"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@mlc-ai/web-llm": "^0.2.79",
    "lit": "^3.3.1"
  },
  "devDependencies": {
    "@types/jsdom": "^27.0.0",
    "@types/node": "^20.10.0",
    "@vitest/ui": "^3.2.4",
    "happy-dom": "^19.0.2",
    "imagemin": "^9.0.1",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^10.0.0",
    "imagemin-webp": "^8.0.0",
    "playwright": "^1.48.0",
    "prettier": "3.6.2",
    "rollup-plugin-visualizer": "^5.12.0",
    "svgo": "^4.0.0",
    "typescript": "^5.9.3",
    "urlpattern-polyfill": "^10.1.0",
    "vite": "^7.1.12",
    "vite-plugin-pwa": "^0.17.0",
    "vitest": "^3.2.4"
  }
}
